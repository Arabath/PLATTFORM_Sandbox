AAA,EAAA,CAAA,WAAA,GAAA,IAAA;AApCA;AAAA;AAAA,8CAsCA,KAAA,CAAA,IAAA,0BAAA,EAAA,CAAA,WAAA,kCACA;AACA,gBAAA,EAAA,EAAA,EAAA,CAAA,SAAA,CAAA,EADA;AAEA,gBAAA,IAAA,EAAA,EAAA,CAAA,SAAA,CAAA;AAFA,eADA,EAKA,aALA,CAtCA;;AAAA;AAsCA,cAAA,QAtCA;AA4CA,cAAA,EAAA,CAAA,WAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,oBAAA,IAAA,CAAA,EAAA,IAAA,QAAA,CAAA,IAAA,CAAA,CAAA,EAAA,EAAA,EAAA;AACA,kBAAA,IAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,CAAA,EAAA,IAAA;AACA;AACA,eAJA;AA5CA;AAAA;;AAAA;AAAA;AAAA;AAkDA,cAAA,OAAA,CAAA,GAAA;;AAlDA;AAAA;AAoDA,cAAA,EAAA,CAAA,WAAA,GAAA,KAAA;AACA,cAAA,EAAA,CAAA,WAAA;AACA,cAAA,EAAA,CAAA,UAAA,CAAA,EAAA,CAAA,WAAA;AAtDA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA0DA,IAAA,4BA1DA;AAAA;AAAA;AAAA;AAAA;AAAA;AA2DA,cAAA,MA3DA,GA2DA;AAAA,gBAAA,aAAA,EAAA,YAAA,KAAA,MAAA,CAAA,KAAA,CAAA;AAAA,eA3DA;AA4DA,cAAA,aA5DA,GA4DA;AAAA,gBAAA,OAAA,EAAA;AAAA,eA5DA;AA6DA,cAAA,EA7DA,GA6DA,IA7DA;AA8DA,cAAA,EAAA,CAAA,WAAA,GAAA,IAAA;AA9DA;AAAA;AAAA,8CAgEA,KAAA,CAAA,IAAA,0BAAA,EAAA,CAAA,WAAA,cAAA,EAAA,CAAA,KAAA,oCACA,EADA,EAGA,aAHA,CAhEA;;AAAA;AAgEA,cAAA,QAhEA;AAoEA,mBAAA,WAAA,GAAA,KAAA,WAAA,CAAA,MAAA,CAAA,UAAA,CAAA;AAAA,uBAAA,CAAA,CAAA,EAAA,KAAA,EAAA,CAAA,KAAA;AAAA,eAAA,CAAA;AACA,cAAA,EAAA,CAAA,UAAA,CAAA,EAAA,CAAA,WAAA;AACA,mBAAA,SAAA,CAAA,OAAA,EACA,8BADA;AAtEA;AAAA;;AAAA;AAAA;AAAA;AAyEA,cAAA,OAAA,CAAA,GAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,aAAA,QAAA,CAAA,IAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,aAAA,QAAA,CAAA,MAAA;;AAEA,kBAAA,aAAA,QAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,qBAAA,SAAA,CAAA,KAAA,EACA,oFADA;AAEA;;AAhFA;AAAA;AAkFA,cAAA,EAAA,CAAA,WAAA,GAAA,KAAA;AACA,cAAA,EAAA,CAAA,kBAAA;AAnFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAuFA,IAAA,0BAvFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAwFA,cAAA,MAxFA,GAwFA;AAAA,gBAAA,aAAA,EAAA,YAAA,KAAA,MAAA,CAAA,KAAA,CAAA;AAAA,eAxFA;AAyFA,cAAA,aAzFA,GAyFA;AAAA,gBAAA,OAAA,EAAA;AAAA,eAzFA;AA0FA,cAAA,EA1FA,GA0FA,IA1FA;AA2FA,cAAA,EAAA,CAAA,WAAA,GAAA,IAAA;AA3FA;AAAA;AAAA,8CA6FA,KAAA,CAAA,GAAA,0BAAA,EAAA,CAAA,WAAA,kCAAA,aAAA,EAAA;AAAA,gBAAA,OAAA,EAAA;AAAA,eAAA,CA7FA;;AAAA;AA6FA,cAAA,QA7FA;AA8FA,cAAA,EAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA;AA9FA;AAAA;;AAAA;AAAA;AAAA;AAiGA,cAAA,OAAA,CAAA,GAAA;;AAjGA;AAAA;AAmGA,cAAA,EAAA,CAAA,WAAA,GAAA,KAAA;AAnGA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAuGA,IAAA,UAvGA,sBAuGA,MAvGA,EAuGA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,YAAA,KAAA,GAAA,CAAA,CAAA,IAAA,CAAA,WAAA,EAAA;AACA,YAAA,KAAA,GAAA,CAAA,CAAA,IAAA,CAAA,WAAA,EAAA;;AACA,YAAA,KAAA,GAAA,KAAA,EAAA;AACA,iBAAA,CAAA,CAAA;AACA;;AACA,YAAA,KAAA,GAAA,KAAA,EAAA;AACA,iBAAA,CAAA;AACA;;AACA,eAAA,CAAA;AACA,OAVA;AAWA,KAnHA;AAqHA,IAAA,SArHA,qBAqHA,IArHA,EAqHA,MArHA,EAqHA;AACA,UAAA,MAAA,IAAA,OAAA,EAAA;AACA,aAAA,SAAA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,SAAA;AACA;;AACA,WAAA,MAAA,GAAA,MAAA;AACA,WAAA,SAAA,GAAA,IAAA,CAAA,MAAA,EAAA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,KA7HA;AA+HA,IAAA,WA/HA,yBA+HA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,eAAA;AACA,WAAA,WAAA;AACA,KAnIA;AAqIA,IAAA,WArIA,yBAqIA;AACA,WAAA,MAAA,GAAA,EAAA;AACA,WAAA,SAAA,GAAA;AACA,QAAA,EAAA,EAAA,EADA;AAEA,QAAA,IAAA,EAAA;AAFA,OAAA;AAIA,KA3IA;AA6IA,IAAA,iBA7IA,6BA6IA,EA7IA,EA6IA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,gBAAA,GAAA,IAAA;AACA,KAhJA;AAkJA,IAAA,kBAlJA,gCAkJA;AACA,WAAA,gBAAA,GAAA,KAAA;AACA,KApJA;AAsJA,IAAA,SAtJA,qBAsJA,IAtJA,EAsJA,OAtJA,EAsJA;AACA,WAAA,QAAA,GAAA,KAAA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,WAAA,YAAA,GAAA,OAAA;AACA,WAAA,QAAA,GAAA,SAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,WAAA,UAAA;AACA,KA7JA;AA+JA,IAAA,UAAA,EAAA,sBAAA;AAAA;;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,EAAA,EAAA,KAAA,CAAA,QAAA,GAAA,KAAA;AACA,OAFA,EAEA,IAFA,CAAA;AAGA;AApKA;AAlDA,CAAA","sourcesContent":["<template>\r\n  <v-layout align-start>\r\n    \r\n    <v-flex>\r\n      <!-- Spinner carga -->\r\n      <v-overlay :value=\"loadingData\">\r\n        <v-progress-circular indeterminate size=\"64\"></v-progress-circular>\r\n      </v-overlay>\r\n\r\n      <v-row>\r\n        <v-col>\r\n          <v-card class=\"px-3 py-1\">\r\n            \r\n            <!-- comienzo data-table {#ff5733} -->\r\n            <v-data-table :headers=\"caberaLibrosTemas\" :items=\"librosTemas\" :search=\"search\" :page.sync=\"page\"\r\n              :items-per-page=\"itemsPerPage\" hide-default-footer @page-count=\"pageCount = $event\" class=\"elevation-1\"\r\n              v-if=\"!loadingData\">\r\n              \r\n              <!-- botones editar y eliminar tema {#e173ff} -->\r\n              <template v-slot:[`item.Acciones`]=\"{ item }\">\r\n                <v-tooltip bottom slot=\"activator\">\r\n                  <template #activator=\"{ on: onTooltip }\">\r\n                    <v-btn color=\"#999999\" v-on=\"onTooltip\" fab x-small dark @click=\"showTCRUD(item,'Editar')\">\r\n                      <v-icon size='16'>mdi-file-document-edit-outline</v-icon>\r\n                    </v-btn>\r\n                  </template>\r\n                  <span class=\"tooltip_small\">Editar Tema</span>\r\n                </v-tooltip>\r\n                &nbsp;&nbsp;\r\n                <v-tooltip bottom slot=\"activator\">\r\n                  <template #activator=\"{ on: onTooltip }\">\r\n                    <v-btn color=\"#ff8888\" v-on=\"onTooltip\" fab x-small dark @click=\"showDeleteConfirm(item.id)\">\r\n                      <v-icon size='16'>mdi-trash-can-outline</v-icon>\r\n                    </v-btn>\r\n                  </template>\r\n                  <span class=\"tooltip_small\">Eliminar Tema</span>\r\n                </v-tooltip>\r\n              </template>\r\n\r\n              <!-- busqueda y nuevo tema {#33ffc1} -->\r\n              <template v-slot:top>\r\n                <v-toolbar flat color=\"white\">\r\n                  <v-toolbar-title>Libros Temas</v-toolbar-title>\r\n                  <v-divider class=\"mx-4\" inset vertical></v-divider>\r\n                  <v-spacer></v-spacer>\r\n                  <v-text-field v-model=\"search\" append-icon=\"mdi-magnify\" label=\"Buscar\" single-line hide-details\r\n                    filled rounded dense>\r\n                  </v-text-field>\r\n                  <v-spacer></v-spacer>\r\n                  <v-tooltip bottom>\r\n                    <template v-slot:activator=\"{ on }\">\r\n                      <v-btn class=\"mx-2\" v-on=\"on\" fab dark small color=\"cyan\" @click=\"showTCRUD(null,'Crear')\"\r\n                        style=\"margin-top: 10px\">\r\n                        <v-icon dark>mdi-pencil</v-icon>\r\n                      </v-btn>\r\n                    </template>\r\n                    <span class=\"tooltip\">Nuevo Tema</span>\r\n                  </v-tooltip>\r\n                </v-toolbar>\r\n              </template>\r\n\r\n              <!-- fin data-table {#ff5733} -->\r\n            </v-data-table>\r\n\r\n            <div class=\"text-center pt-2\">\r\n              <v-pagination v-model=\"page\" :length=\"pageCount\" :total-visible=\"10\"></v-pagination>\r\n            </div>\r\n            \r\n          </v-card>\r\n        </v-col>\r\n      </v-row>\r\n    </v-flex>\r\n\r\n    <!-- ventana dialogo \"nuevo tema\" {#bad1f3} -->\r\n    <v-dialog v-model=\"dialogLTCRUD\" persistent max-width=\"800px\">\r\n      <v-form @submit.prevent=\"enviaDatos()\" ref=\"form\" lazy-validation>\r\n        <LibrosTemasFormCRUD :random_LT=\"random_LT\" :libroTema=\"libroTema\" :accion=\"accion\" @closeLTCRUD=\"closeLTCRUD\"\r\n          @showAlert=\"showAlert\" />\r\n      </v-form>\r\n    </v-dialog>\r\n\r\n    <!-- ventana dialogo para confirmar eliminar tema {#dff3ba} -->\r\n    <v-dialog v-model=\"dlgDeleteConfirm\" persistent max-width=\"600\">\r\n      <v-card class=\"pt-5\" style=\"border:5px solid #ff8888\">\r\n        <v-card-text>\r\n          <v-card-title class=\"headline\">Esta seguro que desea el Tema Seleccionado?</v-card-title>\r\n        </v-card-text>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n          <v-btn text depressed large @click=\"closeDeleteConfirm()\">Cancelar</v-btn>\r\n          <v-btn class=\"my-2\" depressed large color=\"#ff8888\" dark @click=\"EliminarBibliotecaLibrosTema()\"\r\n            style=\"padding-left: 10px; padding-right: 10px\">Eliminar</v-btn>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n\r\n    <template v-if=\"alertDlg\">\r\n      <Alerts :alertColor=\"alertColor\" :alertMessage=\"alertMessage\" :snackbar=\"snackbar\" />\r\n    </template>\r\n\r\n  </v-layout>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\nimport Alerts from \"@/components/Public/Alerts\";\r\nimport LibrosTemasFormCRUD from \"@/components/Biblioteca/LibrosTemasFormCRUD\"\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      page: 1,\r\n      pageCount: 0,\r\n      itemsPerPage: 10,\r\n      search: \"\",\r\n      caberaLibrosTemas: [\r\n        { text: \"ID\", value: \"id\", sortable: false, align: ' d-none', },\r\n        { text: \"Tema\", value: \"tema\" },\r\n        { text: \"\", value: \"Acciones\", sortable: false, align: 'right', },\r\n      ],\r\n      librosTemas: [],\r\n      alertDlg: false,\r\n      loadingData: false,\r\n      random_LT: \"\",\r\n      accion: \"\",\r\n      dialogLTCRUD: false,\r\n      libroTema: {\r\n        id: \"\",\r\n        tema: \"\",\r\n      },\r\n      dlgDeleteConfirm: false,\r\n      selID: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    usuarioID() {\r\n      return this.$store.state.usuario.idusuario;\r\n    },\r\n    institucion() {\r\n      return this.$store.state.usuario.institucion;\r\n    },\r\n    cicloLectivo() {\r\n      if (!this.$store.state.cicloLectivo) {\r\n        return;\r\n      }\r\n      return this.$store.state.cicloLectivo;\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.ListaBibliotecaLibrosTemas();\r\n  },\r\n\r\n  components: {\r\n    Alerts,\r\n    LibrosTemasFormCRUD,\r\n  },\r\n\r\n  methods: {\r\n    async enviaDatos() {\r\n      if (this.accion === 'Crear') {\r\n        const paso1 = await this.CrearBibliotecaLibrosTema();\r\n      }\r\n      else {\r\n        const paso2 = await this.EditarBibliotecaLibrosTema()\r\n      }\r\n      const paso3 = await this.closeLTCRUD();\r\n    },\r\n\r\n    async CrearBibliotecaLibrosTema() {\r\n      let header = { Authorization: \"Bearer \" + this.$store.state.token };\r\n      let configuracion = { headers: header };\r\n      let me = this;\r\n      me.loadingData = true\r\n      try {\r\n        const response = await axios.post(`api/Biblioteca/${me.institucion}/CrearBibliotecaLibrosTema`,\r\n          {\r\n            tema: me.libroTema.tema,\r\n          },\r\n          configuracion);\r\n        me.librosTemas.push(response.data[0])\r\n      } catch (error) {\r\n        console.log(error);\r\n      } finally {\r\n        me.loadingData = false\r\n        me.LimpiaDatos();\r\n        me.sortArrays(me.librosTemas);\r\n      }\r\n    },\r\n\r\n    async EditarBibliotecaLibrosTema() {\r\n      let header = { Authorization: \"Bearer \" + this.$store.state.token };\r\n      let configuracion = { headers: header };\r\n      let me = this;\r\n      me.loadingData = true\r\n      try {\r\n        const response = await axios.post(`api/Biblioteca/${me.institucion}/EditarBibliotecaLibrosTema`,\r\n          {\r\n            id: me.libroTema.id,\r\n            tema: me.libroTema.tema,\r\n          },\r\n          configuracion);\r\n        me.librosTemas.forEach(function (item) {\r\n          if (item.id == response.data[0].id) {\r\n            item.tema = response.data[0].tema;\r\n          }\r\n        });\r\n      } catch (error) {\r\n        console.log(error);\r\n      } finally {\r\n        me.loadingData = false\r\n        me.LimpiaDatos();\r\n        me.sortArrays(me.librosTemas);\r\n      }\r\n    },\r\n\r\n    async EliminarBibliotecaLibrosTema() {\r\n      let header = { Authorization: \"Bearer \" + this.$store.state.token };\r\n      let configuracion = { headers: header };\r\n      let me = this;\r\n      me.loadingData = true\r\n      try {\r\n        const response = await axios.post(`api/Biblioteca/${me.institucion},${me.selID}/EliminarBibliotecaLibrosTema`,\r\n          {\r\n          },\r\n          configuracion);\r\n        this.librosTemas = this.librosTemas.filter((e) => e.id !== me.selID)\r\n        me.sortArrays(me.librosTemas);\r\n        this.showAlert(\"green\",\r\n          \"Registro eliminado con exito\")\r\n      } catch (error) {\r\n        console.log(error);\r\n        console.log(error.response.data);\r\n        console.log(error.response.status);\r\n\r\n        if (error.response.status === 409) {\r\n          this.showAlert(\"red\",\r\n            \"No se pudo eliminar el registro porque esta relacionado con un campo de otra tabla\")\r\n        }\r\n      } finally {\r\n        me.loadingData = false;\r\n        me.closeDeleteConfirm();\r\n      }\r\n    },\r\n\r\n    async ListaBibliotecaLibrosTemas() {\r\n      let header = { Authorization: \"Bearer \" + this.$store.state.token };\r\n      let configuracion = { headers: header };\r\n      let me = this;\r\n      me.loadingData = true\r\n      try {\r\n        const response = await axios.get(`api/Biblioteca/${me.institucion}/ListaBibliotecaLibrosTemas`, configuracion, { timeout: 30000 });\r\n        me.librosTemas = response.data;\r\n\r\n      } catch (error) {\r\n        console.log(error);\r\n      } finally {\r\n        me.loadingData = false\r\n      }\r\n    },\r\n\r\n    sortArrays(arrays) {\r\n      arrays.sort(function (a, b) {\r\n        const temaA = a.tema.toUpperCase();\r\n        const temaB = b.tema.toUpperCase();\r\n        if (temaA < temaB) {\r\n          return -1;\r\n        }\r\n        if (temaA > temaB) {\r\n          return 1;\r\n        }\r\n        return 0;\r\n      });\r\n    },\r\n\r\n    showTCRUD(item, accion) {\r\n      if (accion != 'Crear') {\r\n        this.libroTema = item;\r\n        console.log(this.libroTema)\r\n      }\r\n      this.accion = accion;\r\n      this.random_LT = Math.random();\r\n      this.dialogLTCRUD = true;\r\n    },\r\n\r\n    closeLTCRUD() {\r\n      this.dialogLTCRUD = false;\r\n      this.$refs.form.resetValidation();\r\n      this.LimpiaDatos();\r\n    },\r\n\r\n    LimpiaDatos() {\r\n      this.accion = \"\";\r\n      this.libroTema = {\r\n        id: \"\",\r\n        tema: \"\",\r\n      }\r\n    },\r\n\r\n    showDeleteConfirm(id) {\r\n      this.selID = id;\r\n      this.dlgDeleteConfirm = true;\r\n    },\r\n\r\n    closeDeleteConfirm() {\r\n      this.dlgDeleteConfirm = false;\r\n    },\r\n\r\n    showAlert(tipo, mensaje) {\r\n      this.snackbar = false;\r\n      this.alertColor = tipo;\r\n      this.alertMessage = mensaje;\r\n      this.snackbar = \"mostrar\";\r\n      this.alertDlg = true;\r\n      this.closeAlert();\r\n    },\r\n\r\n    closeAlert: function () {\r\n      var me = this;\r\n      setTimeout(() => {\r\n        (this.snackbar = \"\"), (this.alertDlg = false);\r\n      }, 3000);\r\n    },\r\n  },\r\n\r\n\r\n\r\n\r\n\r\n};\r\n</script>\r\n\r\n"],"sourceRoot":"src/views/Biblioteca"}]}