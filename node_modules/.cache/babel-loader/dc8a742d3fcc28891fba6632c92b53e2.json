,GAoCA,IApCA;AAqCA,cAAA,EAAA,CAAA,WAAA,GAAA,IAAA;AArCA;AAAA;AAAA,8CAuCA,KAAA,CAAA,IAAA,0BAAA,EAAA,CAAA,WAAA,kCACA;AACA,gBAAA,EAAA,EAAA,EAAA,CAAA,SAAA,CAAA,EADA;AAEA,gBAAA,IAAA,EAAA,EAAA,CAAA,SAAA,CAAA;AAFA,eADA,EAKA,aALA,CAvCA;;AAAA;AAuCA,cAAA,QAvCA;AA6CA,cAAA,EAAA,CAAA,WAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,oBAAA,IAAA,CAAA,EAAA,IAAA,QAAA,CAAA,IAAA,CAAA,CAAA,EAAA,EAAA,EAAA;AACA,kBAAA,IAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,CAAA,EAAA,IAAA;AACA;AACA,eAJA;AA7CA;AAAA;;AAAA;AAAA;AAAA;AAmDA,cAAA,OAAA,CAAA,GAAA;;AAnDA;AAAA;AAqDA,cAAA,EAAA,CAAA,WAAA,GAAA,KAAA;AACA,cAAA,EAAA,CAAA,WAAA;AACA,cAAA,EAAA,CAAA,UAAA,CAAA,EAAA,CAAA,WAAA;AAvDA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA2DA,IAAA,4BA3DA;AAAA;AAAA;AAAA;AAAA;AAAA;AA4DA,cAAA,MA5DA,GA4DA;AAAA,gBAAA,aAAA,EAAA,YAAA,KAAA,MAAA,CAAA,KAAA,CAAA;AAAA,eA5DA;AA6DA,cAAA,aA7DA,GA6DA;AAAA,gBAAA,OAAA,EAAA;AAAA,eA7DA;AA8DA,cAAA,EA9DA,GA8DA,IA9DA;AA+DA,cAAA,EAAA,CAAA,WAAA,GAAA,IAAA;AA/DA;AAAA;AAAA,8CAiEA,KAAA,CAAA,IAAA,0BAAA,EAAA,CAAA,WAAA,cAAA,EAAA,CAAA,KAAA,oCACA,EADA,EAGA,aAHA,CAjEA;;AAAA;AAiEA,cAAA,QAjEA;AAqEA,mBAAA,WAAA,GAAA,KAAA,WAAA,CAAA,MAAA,CAAA,UAAA,CAAA;AAAA,uBAAA,CAAA,CAAA,EAAA,KAAA,EAAA,CAAA,KAAA;AAAA,eAAA,CAAA;AACA,cAAA,EAAA,CAAA,UAAA,CAAA,EAAA,CAAA,WAAA;AACA,mBAAA,SAAA,CAAA,OAAA,EACA,8BADA;AAvEA;AAAA;;AAAA;AAAA;AAAA;AA0EA,cAAA,OAAA,CAAA,GAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,aAAA,QAAA,CAAA,IAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,aAAA,QAAA,CAAA,MAAA;;AAEA,kBAAA,aAAA,QAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,qBAAA,SAAA,CAAA,KAAA,EACA,oFADA;AAEA;;AAjFA;AAAA;AAmFA,cAAA,EAAA,CAAA,WAAA,GAAA,KAAA;AACA,cAAA,EAAA,CAAA,kBAAA;AApFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAwFA,IAAA,0BAxFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAyFA,cAAA,MAzFA,GAyFA;AAAA,gBAAA,aAAA,EAAA,YAAA,KAAA,MAAA,CAAA,KAAA,CAAA;AAAA,eAzFA;AA0FA,cAAA,aA1FA,GA0FA;AAAA,gBAAA,OAAA,EAAA;AAAA,eA1FA;AA2FA,cAAA,EA3FA,GA2FA,IA3FA;AA4FA,cAAA,EAAA,CAAA,WAAA,GAAA,IAAA;AA5FA;AAAA;AAAA,8CA8FA,KAAA,CAAA,GAAA,0BAAA,EAAA,CAAA,WAAA,kCAAA,aAAA,EAAA;AAAA,gBAAA,OAAA,EAAA;AAAA,eAAA,CA9FA;;AAAA;AA8FA,cAAA,QA9FA;AA+FA,cAAA,EAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,KAAA,WAAA;AAjGA;AAAA;;AAAA;AAAA;AAAA;AAoGA,cAAA,OAAA,CAAA,GAAA;;AApGA;AAAA;AAsGA,cAAA,EAAA,CAAA,WAAA,GAAA,KAAA;AAtGA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA0GA,IAAA,UA1GA,sBA0GA,MA1GA,EA0GA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,YAAA,KAAA,GAAA,CAAA,CAAA,IAAA,CAAA,WAAA,EAAA;AACA,YAAA,KAAA,GAAA,CAAA,CAAA,IAAA,CAAA,WAAA,EAAA;;AACA,YAAA,KAAA,GAAA,KAAA,EAAA;AACA,iBAAA,CAAA,CAAA;AACA;;AACA,YAAA,KAAA,GAAA,KAAA,EAAA;AACA,iBAAA,CAAA;AACA;;AACA,eAAA,CAAA;AACA,OAVA;AAWA,KAtHA;;AAwHA;AACA,IAAA,SAzHA,qBAyHA,IAzHA,EAyHA,MAzHA,EAyHA;AACA,UAAA,MAAA,IAAA,OAAA,EAAA;AACA,aAAA,SAAA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,SAAA;AACA;;AACA,WAAA,MAAA,GAAA,MAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,WAAA,SAAA,GAAA,IAAA,CAAA,MAAA,EAAA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,KAlIA;AAoIA,IAAA,WApIA,yBAoIA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,eAAA;AACA,WAAA,WAAA;AACA,KAxIA;AA0IA,IAAA,WA1IA,yBA0IA;AACA,WAAA,MAAA,GAAA,EAAA;AACA,WAAA,SAAA,GAAA;AACA,QAAA,EAAA,EAAA,EADA;AAEA,QAAA,IAAA,EAAA;AAFA,OAAA;AAIA,KAhJA;AAkJA,IAAA,iBAlJA,6BAkJA,EAlJA,EAkJA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,gBAAA,GAAA,IAAA;AACA,KArJA;AAuJA,IAAA,kBAvJA,gCAuJA;AACA,WAAA,gBAAA,GAAA,KAAA;AACA,KAzJA;AA2JA,IAAA,SA3JA,qBA2JA,IA3JA,EA2JA,OA3JA,EA2JA;AACA,WAAA,QAAA,GAAA,KAAA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,WAAA,YAAA,GAAA,OAAA;AACA,WAAA,QAAA,GAAA,SAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,WAAA,UAAA;AACA,KAlKA;AAoKA,IAAA,UAAA,EAAA,sBAAA;AAAA;;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,EAAA,EAAA,KAAA,CAAA,QAAA,GAAA,KAAA;AACA,OAFA,EAEA,IAFA,CAAA;AAGA;AAzKA;AAxDA,CAAA","sourcesContent":["<template>\r\n  <v-layout align-start>\r\n    \r\n    <v-flex>\r\n      <!-- Spinner carga -->\r\n      <v-overlay :value=\"loadingData\">\r\n        <v-progress-circular indeterminate size=\"64\"></v-progress-circular>\r\n      </v-overlay>\r\n\r\n      <v-row>\r\n        <v-col>\r\n          <v-card class=\"px-3 py-1\">\r\n            \r\n            <!-- comienzo data-table {#ff5733} -->\r\n            <v-data-table :headers=\"caberaLibrosTemas\" :items=\"librosTemas\" :search=\"search\" :page.sync=\"page\"\r\n              :items-per-page=\"itemsPerPage\" hide-default-footer @page-count=\"pageCount = $event\" class=\"elevation-1\"\r\n              v-if=\"!loadingData\">\r\n              \r\n              <!-- botones editar y eliminar tema {#e173ff} -->\r\n              <template v-slot:[`item.Acciones`]=\"{ item }\">\r\n                <v-tooltip bottom slot=\"activator\">\r\n                  <template #activator=\"{ on: onTooltip }\">\r\n                    <v-btn color=\"#999999\" v-on=\"onTooltip\" fab x-small dark @click=\"showTCRUD(item,'Editar')\">\r\n                      <v-icon size='16'>mdi-file-document-edit-outline</v-icon>\r\n                    </v-btn>\r\n                  </template>\r\n                  <span class=\"tooltip_small\">Editar Tema</span>\r\n                </v-tooltip>\r\n                &nbsp;&nbsp;\r\n                <v-tooltip bottom slot=\"activator\">\r\n                  <template #activator=\"{ on: onTooltip }\">\r\n                    <v-btn color=\"#ff8888\" v-on=\"onTooltip\" fab x-small dark @click=\"showDeleteConfirm(item.id)\">\r\n                      <v-icon size='16'>mdi-trash-can-outline</v-icon>\r\n                    </v-btn>\r\n                  </template>\r\n                  <span class=\"tooltip_small\">Eliminar Tema</span>\r\n                </v-tooltip>\r\n              </template>\r\n\r\n              <!-- busqueda  -->\r\n              <template v-slot:top>\r\n                <v-toolbar flat color=\"white\">\r\n                  <v-toolbar-title>Libros Temas</v-toolbar-title>\r\n                  <v-divider class=\"mx-4\" inset vertical></v-divider>\r\n                  <v-spacer></v-spacer>\r\n                  <v-text-field v-model=\"search\" append-icon=\"mdi-magnify\" label=\"Buscar\" single-line hide-details\r\n                    filled rounded dense>\r\n                  </v-text-field>\r\n                  <v-spacer></v-spacer>\r\n                  \r\n                  <!-- Nuevo Tema Btn /*{#ff3399}*/ -->\r\n                  <v-tooltip bottom>\r\n                    <template v-slot:activator=\"{ on }\">\r\n                      <v-btn class=\"mx-2\" v-on=\"on\" fab dark small color=\"cyan\" @click=\"showTCRUD(null,'Crear')\"\r\n                        style=\"margin-top: 10px\">\r\n                        <v-icon dark>mdi-pencil</v-icon>\r\n                      </v-btn>\r\n                    </template>\r\n                    <span class=\"tooltip\">Nuevo Tema</span>\r\n                  </v-tooltip>\r\n                </v-toolbar>\r\n              </template>\r\n\r\n              <!-- fin data-table {#ff5733} -->\r\n            </v-data-table>\r\n\r\n            <div class=\"text-center pt-2\">\r\n              <v-pagination v-model=\"page\" :length=\"pageCount\" :total-visible=\"10\"></v-pagination>\r\n            </div>\r\n            \r\n          </v-card>\r\n        </v-col>\r\n      </v-row>\r\n    </v-flex>\r\n\r\n    <!-- ventana dialogo \"nuevo tema\" /*{#bad1f3}*/ -->\r\n    <v-dialog v-model=\"dialogLTCRUD\" persistent max-width=\"800px\">\r\n      <v-form @submit.prevent=\"enviaDatos()\" ref=\"form\" lazy-validation>\r\n        <LibrosTemasFormCRUD :random_LT=\"random_LT\" :libroTema=\"libroTema\" :accion=\"accion\" @closeLTCRUD=\"closeLTCRUD\"\r\n          @showAlert=\"showAlert\" />\r\n      </v-form>\r\n    </v-dialog>\r\n\r\n    <!-- ventana dialogo para confirmar eliminar tema {#dff3ba} -->\r\n    <v-dialog v-model=\"dlgDeleteConfirm\" persistent max-width=\"600\">\r\n      <v-card class=\"pt-5\" style=\"border:5px solid #ff8888\">\r\n        <v-card-text>\r\n          <v-card-title class=\"headline\">Esta seguro que desea el Tema Seleccionado?</v-card-title>\r\n        </v-card-text>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n          <v-btn text depressed large @click=\"closeDeleteConfirm()\">Cancelar</v-btn>\r\n          <v-btn class=\"my-2\" depressed large color=\"#ff8888\" dark @click=\"EliminarBibliotecaLibrosTema()\"\r\n            style=\"padding-left: 10px; padding-right: 10px\">Eliminar</v-btn>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n\r\n    <template v-if=\"alertDlg\">\r\n      <Alerts :alertColor=\"alertColor\" :alertMessage=\"alertMessage\" :snackbar=\"snackbar\" />\r\n    </template>\r\n\r\n  </v-layout>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\nimport Alerts from \"@/components/Public/Alerts\";\r\nimport LibrosTemasFormCRUD from \"@/components/Biblioteca/LibrosTemasFormCRUD\"\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      page: 1,\r\n      pageCount: 0,\r\n      itemsPerPage: 10,\r\n      search: \"\",\r\n      caberaLibrosTemas: [\r\n        { text: \"ID\", value: \"id\", sortable: false, align: ' d-none', },\r\n        { text: \"Tema\", value: \"tema\" },\r\n        { text: \"\", value: \"Acciones\", sortable: false, align: 'right', },\r\n      ],\r\n      librosTemas: [],\r\n      alertDlg: false,\r\n      loadingData: false,\r\n\r\n      /* Props Strings {#c6ed97} */\r\n      random_LT: \"\",\r\n      accion: \"\",\r\n      \r\n      dialogLTCRUD: false,\r\n\r\n      /* objeto libroTema {#c6ed97} */\r\n      libroTema: {\r\n        id: \"\",\r\n        tema: \"\",\r\n      },\r\n\r\n      dlgDeleteConfirm: false,\r\n      selID: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    usuarioID() {\r\n      return this.$store.state.usuario.idusuario;\r\n    },\r\n    institucion() {\r\n      return this.$store.state.usuario.institucion;\r\n    },\r\n    cicloLectivo() {\r\n      if (!this.$store.state.cicloLectivo) {\r\n        return;\r\n      }\r\n      return this.$store.state.cicloLectivo;\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.ListaBibliotecaLibrosTemas();\r\n  },\r\n\r\n  components: {\r\n    Alerts,\r\n    LibrosTemasFormCRUD,\r\n  },\r\n\r\n  methods: {\r\n\r\n    async enviaDatos() {\r\n      if (this.accion === 'Crear') {\r\n        const paso1 = await this.CrearBibliotecaLibrosTema();\r\n      }\r\n      else {\r\n        const paso2 = await this.EditarBibliotecaLibrosTema()\r\n      }\r\n      const paso3 = await this.closeLTCRUD();\r\n    },\r\n\r\n    async CrearBibliotecaLibrosTema() {\r\n      let header = { Authorization: \"Bearer \" + this.$store.state.token };\r\n      let configuracion = { headers: header };\r\n      let me = this;\r\n      me.loadingData = true\r\n      try {\r\n        const response = await axios.post(`api/Biblioteca/${me.institucion}/CrearBibliotecaLibrosTema`,\r\n          {\r\n            tema: me.libroTema.tema,\r\n          },\r\n          configuracion);\r\n        me.librosTemas.push(response.data[0])\r\n      } catch (error) {\r\n        console.log(error);\r\n      } finally {\r\n        me.loadingData = false\r\n        me.LimpiaDatos();\r\n        me.sortArrays(me.librosTemas);\r\n      }\r\n    },\r\n\r\n    async EditarBibliotecaLibrosTema() {\r\n      let header = { Authorization: \"Bearer \" + this.$store.state.token };\r\n      let configuracion = { headers: header };\r\n      let me = this;\r\n      me.loadingData = true\r\n      try {\r\n        const response = await axios.post(`api/Biblioteca/${me.institucion}/EditarBibliotecaLibrosTema`,\r\n          {\r\n            id: me.libroTema.id,\r\n            tema: me.libroTema.tema,\r\n          },\r\n          configuracion);\r\n        me.librosTemas.forEach(function (item) {\r\n          if (item.id == response.data[0].id) {\r\n            item.tema = response.data[0].tema;\r\n          }\r\n        });\r\n      } catch (error) {\r\n        console.log(error);\r\n      } finally {\r\n        me.loadingData = false\r\n        me.LimpiaDatos();\r\n        me.sortArrays(me.librosTemas);\r\n      }\r\n    },\r\n\r\n    async EliminarBibliotecaLibrosTema() {\r\n      let header = { Authorization: \"Bearer \" + this.$store.state.token };\r\n      let configuracion = { headers: header };\r\n      let me = this;\r\n      me.loadingData = true\r\n      try {\r\n        const response = await axios.post(`api/Biblioteca/${me.institucion},${me.selID}/EliminarBibliotecaLibrosTema`,\r\n          {\r\n          },\r\n          configuracion);\r\n        this.librosTemas = this.librosTemas.filter((e) => e.id !== me.selID)\r\n        me.sortArrays(me.librosTemas);\r\n        this.showAlert(\"green\",\r\n          \"Registro eliminado con exito\")\r\n      } catch (error) {\r\n        console.log(error);\r\n        console.log(error.response.data);\r\n        console.log(error.response.status);\r\n\r\n        if (error.response.status === 409) {\r\n          this.showAlert(\"red\",\r\n            \"No se pudo eliminar el registro porque esta relacionado con un campo de otra tabla\")\r\n        }\r\n      } finally {\r\n        me.loadingData = false;\r\n        me.closeDeleteConfirm();\r\n      }\r\n    },\r\n\r\n    async ListaBibliotecaLibrosTemas() {\r\n      let header = { Authorization: \"Bearer \" + this.$store.state.token };\r\n      let configuracion = { headers: header };\r\n      let me = this;\r\n      me.loadingData = true\r\n      try {\r\n        const response = await axios.get(`api/Biblioteca/${me.institucion}/ListaBibliotecaLibrosTemas`, configuracion, { timeout: 30000 });\r\n        me.librosTemas = response.data;\r\n        console.log(response.data)\r\n        console.log(this.librosTemas)\r\n\r\n      } catch (error) {\r\n        console.log(error);\r\n      } finally {\r\n        me.loadingData = false\r\n      }\r\n    },\r\n\r\n    sortArrays(arrays) {\r\n      arrays.sort(function (a, b) {\r\n        const temaA = a.tema.toUpperCase();\r\n        const temaB = b.tema.toUpperCase();\r\n        if (temaA < temaB) {\r\n          return -1;\r\n        }\r\n        if (temaA > temaB) {\r\n          return 1;\r\n        }\r\n        return 0;\r\n      });\r\n    },\r\n\r\n    /* funcion para mostrar ventana {#ff3399} */\r\n    showTCRUD(item, accion) {\r\n      if (accion != 'Crear') {\r\n        this.libroTema = item;\r\n        console.log(this.libroTema)\r\n      }\r\n      this.accion = accion;\r\n      console.log(accion)\r\n      this.random_LT = Math.random();\r\n      this.dialogLTCRUD = true;\r\n    },\r\n\r\n    closeLTCRUD() {\r\n      this.dialogLTCRUD = false;\r\n      this.$refs.form.resetValidation();\r\n      this.LimpiaDatos();\r\n    },\r\n\r\n    LimpiaDatos() {\r\n      this.accion = \"\";\r\n      this.libroTema = {\r\n        id: \"\",\r\n        tema: \"\",\r\n      }\r\n    },\r\n\r\n    showDeleteConfirm(id) {\r\n      this.selID = id;\r\n      this.dlgDeleteConfirm = true;\r\n    },\r\n\r\n    closeDeleteConfirm() {\r\n      this.dlgDeleteConfirm = false;\r\n    },\r\n\r\n    showAlert(tipo, mensaje) {\r\n      this.snackbar = false;\r\n      this.alertColor = tipo;\r\n      this.alertMessage = mensaje;\r\n      this.snackbar = \"mostrar\";\r\n      this.alertDlg = true;\r\n      this.closeAlert();\r\n    },\r\n\r\n    closeAlert: function () {\r\n      var me = this;\r\n      setTimeout(() => {\r\n        (this.snackbar = \"\"), (this.alertDlg = false);\r\n      }, 3000);\r\n    },\r\n  },\r\n\r\n\r\n\r\n\r\n\r\n};\r\n</script>\r\n\r\n"],"sourceRoot":"src/views/Biblioteca"}]}