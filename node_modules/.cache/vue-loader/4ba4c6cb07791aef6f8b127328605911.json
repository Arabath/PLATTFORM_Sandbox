,\r\n  created() {\r\n    if (this.personas.length > 0) {\r\n      if (this.alert.estado === true) {\r\n        this.showAlert(this.alert.tipo, this.alert.mensaje);\r\n      }\r\n      return;\r\n    }\r\n    this.cargarPersonas();\r\n\r\n    this.mostrarLoading({\r\n      titulo: \"Accediendo a la informaci√≥n\",\r\n      color: \"#90caf9\",\r\n      loader: \"ring-loader\",\r\n      loadercolor: \"#fff\",\r\n    });\r\n  },\r\n  components: {\r\n    Alerts,\r\n  },\r\n};\r\n</script>\r\n<style>\r\n\r\n#create .v-speed-dial {\r\n  position: absolute;\r\n}\r\n\r\n#create .v-btn--floating {\r\n  position: relative;\r\n}\r\n</style>"]}]}